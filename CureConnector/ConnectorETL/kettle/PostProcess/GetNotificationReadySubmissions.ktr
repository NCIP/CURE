    <connection>jndi_gateway_etl</connection>
    <sql>SELECT 
  sub.idx
, sub.client_id
, sub.client_name
, sub.property_file_name
, sub.xml_file_count
, sub.start_ts
, sub.end_ts
, sub.status
, sub.description
, sub.notification_email
, tran.status as tran_status
,case when( tran.status is null or (tran.status is not null and (tran.status &gt;= 80 or (tran.status+1)%10=0))) and
          (pretran.status = 30 or (pretran.status+1)%10=0) then &apos;PROCESSED&apos; else &apos;IN_PROGRESS&apos; end as &quot;submission_new_status&quot;
, count(pretran.idx)
FROM fileprocess.submissions sub, fileprocess.pre_transactions pretran
left join fileprocess.transactions tran on pretran.idx = tran.ptxn_id
where pretran.submmission_id = sub.idx
and sub.status != &apos;PROCESSED_WITH_NOTIFICATION&apos;
group by sub.idx, sub.client_id, sub.client_name, sub.property_file_name, sub.xml_file_count, sub.start_ts, sub.end_ts, sub.status, sub.description, sub.notification_email, 
tran.idx, tran.status, pretran.status</sql>
    <limit>0</limit>
