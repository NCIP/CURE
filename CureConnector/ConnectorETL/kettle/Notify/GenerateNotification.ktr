    <sql>select distinct p.status as &quot;preTransactionStatus&quot;, 
t.status as &quot;transactionStatus&quot;, 
t.partial_complete as &quot;partialComplete&quot;,
p.idx as &quot;pre_transaction_id&quot;,
p.orig_xml_file_name as &quot;inputFile&quot;,
p.xml_file_name as &quot;baseInputFile&quot;,
s.status_description as &quot;transactionStatusDescription&quot;,
s2.status_description as &quot;preTransactionStatusDescription&quot;,
pd.key_field as &quot;preTransactionKey&quot;,
pd.value_field as &quot;preTransactionValue&quot;,
pd.error_description as &quot;preTransactionError&quot;,
d.description as &quot;transactionError&quot;,
t.form_id as &quot;transactionFormId&quot;,
t.instance_index as &quot;transactionInstanceIndex&quot;,
t.parent_instance_index as &quot;transactionParentInstanceIndex&quot;,
t.idx as &quot;transactionId&quot;,
(select case when max(status) &gt;= 90 then TRUE else FALSE end from fileprocess.transactions where ptxn_id=?) as &quot;anyDataMigrationForInputFile&quot;,
(select exists (select description from fileprocess.transactions_detail where txn_id in (select idx from fileprocess.transactions where ptxn_id=?))) as &quot;anyTransactionErrors&quot;,
(select min(idx) = t.idx from fileprocess.transactions where ptxn_id=? and form_id=t.form_id and instance_index = t.instance_index) as &quot;firstRowForTransaction&quot;,
(select min(instance_index)=t.instance_index and min(form_id)=t.form_id and min(idx) = t.idx from fileprocess.transactions where ptxn_id=?) as &quot;firstRowForInputFile&quot;,
(p.status &lt; 30) as &quot;noTransactions&quot;
from &quot;fileprocess&quot;.status s2, 
&quot;fileprocess&quot;.pre_transactions p 
left join &quot;fileprocess&quot;.transactions t on t.ptxn_id = p.idx
left join &quot;fileprocess&quot;.status s on t.status = s.status
left join &quot;fileprocess&quot;.pre_transaction_details pd on pd.pre_transaction_id = p.idx
left join &quot;fileprocess&quot;.transactions_detail d on d.txn_id = t.idx
where p.submmission_id = ?
and p.status = s2.status
and p.idx = ?
order by p.idx, p.orig_xml_file_name, t.form_id, t.instance_index, t.idx</sql>
