    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>

var emailContent = &apos;&apos;;
var mail_part_type = 0;
var hasValidationErrors = preTransactionStatus &lt; 30;
var hasDataMigrationErrors = (transactionStatus!=null &amp;&amp; transactionStatus &lt; 90 &amp;&amp; (transactionStatus+1)%10 == 0) || (transactionError != null);

if ( firstRowForInputFile || noTransactions ) &#47;&#47; This means that the current row is the very first row in the resultset for the submitted input file
{
    mail_part_type = 1;
    emailContent = &apos;\nInput File: &apos; + (inputFile == null ? baseInputFile : inputFile);
    emailContent += &apos;---------------------------------------------------------------------------&apos;;

    if (transactionStatus==null &amp;&amp; anyDataMigrationForInputFile==false)
    {
	   emailContent += &apos;\nRESULT: Data migration could not be completed.\n&apos;;
    }
    else if (transactionStatus==null &amp;&amp; anyDataMigrationForInputFile==true)
    {
	   emailContent += &apos;\nRESULT: Data migration was partially completed.\nOnly a portion of the data was migrated successfully.\n&apos;;
    }
    else if (anyTransactionErrors==true)
    {
       emailContent += &apos;\nRESULT: Data migration was partially completed.\nOnly a portion of the data was migrated successfully.\n&apos;;
    }
    else if (transactionStatus &gt;= 90 &amp;&amp; partialComplete == true) 
    {
	   emailContent += &apos;\nRESULT: Data migration was partially completed.\nOnly a portion of the data was migrated successfully.\n&apos;;
    }
    else if (transactionStatus &gt;= 90 &amp;&amp; partialComplete == false)
    {
	   emailContent += &apos;\nRESULT: Data migration was completed successfully!\n&apos;;
    }

    if ( hasValidationErrors || hasDataMigrationErrors )
    {
        emailContent +=&apos;\nData migration could not be completely processed due to:\n&apos;;
    	if ( hasValidationErrors ) emailContent += &apos;-Validation Errors\n\n&apos;;
    	if ( hasDataMigrationErrors ) emailContent += &apos;-Issues Encountered During Data Migration\n\n&apos;;

        if (preTransactionStatus == 9){
	      emailContent += &apos;ERROR: Could not find input file &apos; +  baseInputFile;
          emailContent += &apos;\nError Code: &apos; + preTransactionStatusDescription + &apos;\n\n&apos;;
       }
       else if (preTransactionStatus == 19)
	      emailContent += &apos;ERROR: XSD validation has failed:\nError Code: &apos; + preTransactionStatusDescription + &apos;\n\n&apos;;
       else if (preTransactionStatus == 29)
	      emailContent += &apos;ERROR: Data validation has failed:\nError Code: &apos; + preTransactionStatusDescription + &apos;\n\n&apos;;
    }
}

if ( firstRowForTransaction == true ) &#47;&#47; This means that we have started processing a new individual transaction for this input file 
                              &#47;&#47; (a transaction represents one form instance, and an input file may consist of several form instances)

{

    if ( hasDataMigrationErrors == true )
    {
       if ( !firstRowForInputFile ) mail_part_type = 2;    &#47;&#47; No need to reset mail_part_type if it is the very first row for this input file
                                                 &#47;&#47; (because mail_part_type will have already been set)

       if ( transactionStatus == 39 ){
	      emailContent += &apos;ERROR: Splitting up file into multiple forms has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;
       }
       else if (transactionStatus == 49 ){
	      emailContent += &apos;ERROR: Conversion of the XML input file has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;

       }
       else if (transactionStatus == 59 ){
	      emailContent += &apos;ERROR: Loading of Collector form template has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;
       }
       else if (transactionStatus == 69 ){
	      emailContent += &apos;ERROR: XML Merge has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;
       }
       else if (transactionStatus == 79 ){
	      emailContent += &apos;ERROR: CouchDB upload has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;
       }
       else if (transactionStatus == 89 ){
	      emailContent += &apos;ERROR: CaCURE update has failed\nError Code: &apos; + transactionStatusDescription + &apos;\n&apos;;
          emailContent += &apos;\nForm ID: &apos; + transactionFormId + &apos;\nInstance Index: &apos; + transactionInstanceIndex + &apos;\n&apos;;
          emailContent += &apos;\nParent Instance Index: &apos; + (transactionParentInstanceIndex != null ? transactionParentInstanceIndex : &apos;NONE&apos;) + &apos;\n&apos;;
		  emailContent += &apos;\n&apos;;
       }
       else
          emailContent += &apos;ERROR: Processing of the XML input file has failed\n\n&apos;;
    }
}

var hasPreTransactionKeyAndValue = (preTransactionKey!=null &amp;&amp; preTransactionValue!=null );
var hasPreTransactionDetails = hasPreTransactionKeyAndValue || (preTransactionError != null);
var hasTransactionDetails = (transactionError != null);

if ( hasPreTransactionDetails == true ) 
{
    if ( !firstRowForInputFile &amp;&amp; !firstRowForTransaction ) mail_part_type = 3; &#47;&#47; No need to set mail_part_type if it is the first row for this input file, or for this transaction
                                                                      &#47;&#47; (because mail_part_type will have already been set)

    emailContent += &apos;\nERROR: &apos; + ( preTransactionError==null ? &apos;&apos; : preTransactionError );
    if ( hasPreTransactionKeyAndValue )
    {
          emailContent +=&apos;\nQuestion: &apos; + preTransactionKey;
          emailContent +=&apos;\nAnswer Value: &apos; + preTransactionValue;
    }
}

if ( hasTransactionDetails == true )
{
    if ( !firstRowForInputFile &amp;&amp; !firstRowForTransaction ) mail_part_type = 3; &#47;&#47; No need to set mail_part_type if it is the first row for this input file, or for this transaction
                                                                           &#47;&#47; (because mail_part_type will have already been set)

    emailContent += &apos;\n\nERROR: &apos; + transactionError;
    if ( transactionFormId != null ) emailContent += &apos;\nForm ID: &apos; + transactionFormId;
    if ( transactionInstanceIndex != null ) emailContent += &apos;\nInstance Index: &apos; + transactionInstanceIndex;
    if ( transactionParentInstanceIndex != null ) emailContent += &apos;\nParent Instance Index: &apos; + transactionParentInstanceIndex;
}
